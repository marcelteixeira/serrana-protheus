	''																		, ; //X2_USROBJ
	''																		, ; //X2_POSLGT
	''																		, ; //X2_CLOB
	''																		, ; //X2_AUTREC
	'E'																		, ; //X2_MODOEMP
	'E'																		, ; //X2_MODOUN
	0																		} ) //X2_MODULO

//
// Tabela SZ1
//
aAdd( aSX2, { ;
	cCtbaTab+'1'																	, ; //X2_CHAVE
	cPath																	, ; //X2_PATH
	cCtbaTab+'1'+cEmpr																, ; //X2_ARQUIVO
	'REGRA CONTABIL FINANCEIRA'												, ; //X2_NOME
	'REGRA CONTABIL FINANCEIRA'												, ; //X2_NOMESPA
	'REGRA CONTABIL FINANCEIRA'												, ; //X2_NOMEENG
