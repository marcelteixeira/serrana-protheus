	cPath																	, ; //X2_PATH
	cCtbaTab+'3'+cEmpr																, ; //X2_ARQUIVO
	'REGRA CONTABIL FOLHA PGTO'												, ; //X2_NOME
	'REGRA CONTABIL FOLHA PGTO'												, ; //X2_NOMESPA
	'REGRA CONTABIL FOLHA PGTO'												, ; //X2_NOMEENG
	'E'																		, ; //X2_MODO
	''																		, ; //X2_TTS
	''																		, ; //X2_ROTINA
	''																		, ; //X2_PYME
	''																		, ; //X2_UNICO
	''																		, ; //X2_DISPLAY
	''																		, ; //X2_SYSOBJ
	''																		, ; //X2_USROBJ
	''																		, ; //X2_POSLGT
	''																		, ; //X2_CLOB
	''																		, ; //X2_AUTREC
	'E'																		, ; //X2_MODOEMP
	'E'																		, ; //X2_MODOUN
	0																		} ) //X2_MODULO

//
// Tabela SZ4
//
aAdd( aSX2, { ;
	cCtbaTab+'4'																	, ; //X2_CHAVE
	cPath																	, ; //X2_PATH
	cCtbaTab+'4'+cEmpr																, ; //X2_ARQUIVO
	'REGRA CONTABIL GENERICA'												, ; //X2_NOME
	'REGRA CONTABIL GENERICA'												, ; //X2_NOMESPA
	'REGRA CONTABIL GENERICA'												, ; //X2_NOMEENG
	'E'																		, ; //X2_MODO
	''																		, ; //X2_TTS
	''																		, ; //X2_ROTINA
	''																		, ; //X2_PYME
	''																		, ; //X2_UNICO
	cCtbaCpo+'4_SIGLA+'+cCtbaCpo+'4_SEQUEN'													, ; //X2_DISPLAY
	''																		, ; //X2_SYSOBJ
	''																		, ; //X2_USROBJ
	''																		, ; //X2_POSLGT
	''																		, ; //X2_CLOB
	''																		, ; //X2_AUTREC
